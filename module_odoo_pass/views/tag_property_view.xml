<odoo>
    <data>
        <record id="tag_property_view_form" model="ir.ui.view">
            <field name="name">tag_property_view_form</field>
            <field name="model">tag_property.state</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>

        <record id="tag_property_view_tree" model="ir.ui.view">
            <field name="name">tag_property_view_tree</field>
            <field name="model">tag_property.state</field>
            <field name="arch" type="xml">
                <tree editable= 'bottom'>
                    <field name="name"/>
                </tree>
            </field>
        </record>
    </data>
</odoo>
